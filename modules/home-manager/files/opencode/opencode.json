  "lsp": {
    "clojure-lsp": {
      "command": ["clojure-lsp", "listen"],
      "extensions": [".clj", ".cljs", ".cljc", ".edn"]
    }
  },
  "mcp": {
    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "-y", "chrome-devtools-mcp@latest"],
      "enabled": false
    },
    "devenv": {
      "type": "local",
      "command": ["devenv", "mcp"],
      "enabled": false,
      "environment": {
        "DEVENV_ROOT": "config.devenv.root"
      }
    },
    "backlog": {
      "type": "local",
      "command": ["backlog", "mcp", "start"],
      "enabled": false
    }
  },
  "tui": {
    "scroll_speed": 3,
    "scroll_acceleration": {
      "enabled": true
    }
  }
